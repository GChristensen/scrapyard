{
    "manifest_version": 2,
    "name": "Scrapyard",
    "version": "0.0.85",
    "description": "__MSG_extensionDescription__",
    "default_locale": "en",
    "icons": {
	"64": "icons/scrapyard.svg"
    },
    "content_security_policy": "script-src 'self'; object-src 'self'; navigation-to 'filesystem:'",
    "permissions": [
        "activeTab",
        "bookmarks",
        "contextMenus",
        "downloads",
        "file:///*",
        "http://*/*",
        "https://*/*",
        "menus",
        "nativeMessaging",
        "notifications",
        "storage",
        "tabs",
        "unlimitedStorage",
        "webNavigation",
        "webRequest",
        "webRequestBlocking"
    ],
    "applications": {
        "gecko": {
            "id": "scrapyard@firefox",
            "strict_min_version": "60.0"
        }
    },
    "sidebar_action": {
        "default_title": "Scrapyard",
        "default_panel": "sidebar.html",
        "default_icon": "icons/scrapyard.svg"
    },
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": {
            "64": "icons/scrapyard.svg"
        },
        "default_title": "Scrapyard",
        "default_popup": "popup.html"
    },
    "options_ui": {
        "open_in_tab": true,
        "page": "options.html"
    },
    "web_accessible_resources": ["icons/lock.svg"]
}
